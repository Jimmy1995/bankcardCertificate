package com.x2x.api.vo.ssbankcard;

import com.x2x.api.vo.BasicReq;
/**
 * 
 * @DESC 长沙松顺银行卡实名认证请求参数对象 
 * @AUTHOR JIANGCW
 * @DATE 2018-1-10下午02:16:30
 */
public class SsCommonRequest extends BasicReq<SsCommonResponse>{
	private static final long serialVersionUID = -1218836844657379446L;
	private String mChild;//下游商户号
	//下游交易流水号
	private String downTransNo;
	//服务注册时定义的服务版本名，默认1.0
	private String version;
	//调用方注册的账户ID
	private String accCode;
	//用户申请的密钥的索引
	private String accessKeyId;
	//签名结果串
	private String signature;
	//请求的时间戳。值为当前距离1970年1月1日原点的毫秒数
	private String timestamp;
	
	//商户流水号（唯一），不超过32位
	private String requestId;
	//认证模式(303:银行卡四要素实名认证接口,302:银行卡三要素实名认证接口,301:银行卡二要素实名认证接口,312:银行卡三要素精准错误返回实名认证接口,313:银行卡四要素精准错误返回实名认证接口)
	private String serviceCode;
	//银行卡号码
	private String bankCard;
	//姓名
	private String name;
	//身份证号码
	private String idNumber;
	//手机号码
	private String mobile;
	//商户号
	private String merchantId;
	
	public String getmChild() {
		return mChild;
	}
	public void setmChild(String mChild) {
		this.mChild = mChild;
	}
	public String getDownTransNo() {
		return downTransNo;
	}
	public void setDownTransNo(String downTransNo) {
		this.downTransNo = downTransNo;
	}
	public String getVersion() {
		return version;
	}
	public void setVersion(String version) {
		this.version = version;
	}
	public String getAccCode() {
		return accCode;
	}
	public void setAccCode(String accCode) {
		this.accCode = accCode;
	}
	public String getAccessKeyId() {
		return accessKeyId;
	}
	public void setAccessKeyId(String accessKeyId) {
		this.accessKeyId = accessKeyId;
	}
	public String getSignature() {
		return signature;
	}
	public void setSignature(String signature) {
		this.signature = signature;
	}
	public String getTimestamp() {
		return timestamp;
	}
	public void setTimestamp(String timestamp) {
		this.timestamp = timestamp;
	}
	public String getRequestId() {
		return requestId;
	}
	public void setRequestId(String requestId) {
		this.requestId = requestId;
	}
	public String getServiceCode() {
		return serviceCode;
	}
	public void setServiceCode(String serviceCode) {
		this.serviceCode = serviceCode;
	}
	public String getBankCard() {
		return bankCard;
	}
	public void setBankCard(String bankCard) {
		this.bankCard = bankCard;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getIdNumber() {
		return idNumber;
	}
	public void setIdNumber(String idNumber) {
		this.idNumber = idNumber;
	}
	public String getMobile() {
		return mobile;
	}
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	public String getMerchantId() {
		return merchantId;
	}
	public void setMerchantId(String merchantId) {
		this.merchantId = merchantId;
	}
	
}
