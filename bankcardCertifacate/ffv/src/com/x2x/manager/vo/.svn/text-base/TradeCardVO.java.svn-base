package com.x2x.manager.vo;

import java.io.Serializable;

public class TradeCardVO implements Serializable{
	private static final long serialVersionUID = 2219681096789945458L;
	private String srcRealName;//户名
	private String srcIdCard;//身份证
	private String srcBankCard;//银行卡
	private String srcBankPhone;//预留手机号
	private String srcExpired;//有效期
	private String srcCvv2;//
	private String targetRealName;//户名
	private String targetIdCard;//身份证
	private String targetBankCard;//银行卡
	private String targetBankPhone;//预留手机号
	private String targetBankName;//银行名
	public String getSrcRealName() {
		return srcRealName;
	}
	public void setSrcRealName(String srcRealName) {
		this.srcRealName = srcRealName;
	}
	public String getSrcIdCard() {
		return srcIdCard;
	}
	public void setSrcIdCard(String srcIdCard) {
		this.srcIdCard = srcIdCard;
	}
	public String getSrcBankCard() {
		return srcBankCard;
	}
	public void setSrcBankCard(String srcBankCard) {
		this.srcBankCard = srcBankCard;
	}
	public String getSrcBankPhone() {
		return srcBankPhone;
	}
	public void setSrcBankPhone(String srcBankPhone) {
		this.srcBankPhone = srcBankPhone;
	}
	public String getSrcExpired() {
		return srcExpired;
	}
	public void setSrcExpired(String srcExpired) {
		this.srcExpired = srcExpired;
	}
	public String getSrcCvv2() {
		return srcCvv2;
	}
	public void setSrcCvv2(String srcCvv2) {
		this.srcCvv2 = srcCvv2;
	}
	public String getTargetRealName() {
		return targetRealName;
	}
	public void setTargetRealName(String targetRealName) {
		this.targetRealName = targetRealName;
	}
	public String getTargetIdCard() {
		return targetIdCard;
	}
	public void setTargetIdCard(String targetIdCard) {
		this.targetIdCard = targetIdCard;
	}
	public String getTargetBankCard() {
		return targetBankCard;
	}
	public void setTargetBankCard(String targetBankCard) {
		this.targetBankCard = targetBankCard;
	}
	public String getTargetBankPhone() {
		return targetBankPhone;
	}
	public void setTargetBankPhone(String targetBankPhone) {
		this.targetBankPhone = targetBankPhone;
	}
	public String getTargetBankName() {
		return targetBankName;
	}
	public void setTargetBankName(String targetBankName) {
		this.targetBankName = targetBankName;
	}
	public TradeCardVO(String srcRealName, String srcIdCard,
			String srcBankCard, String srcBankPhone, String srcExpired,
			String srcCvv2, String targetRealName, String targetIdCard,
			String targetBankCard, String targetBankPhone, String targetBankName) {
		super();
		this.srcRealName = srcRealName;
		this.srcIdCard = srcIdCard;
		this.srcBankCard = srcBankCard;
		this.srcBankPhone = srcBankPhone;
		this.srcExpired = srcExpired;
		this.srcCvv2 = srcCvv2;
		this.targetRealName = targetRealName;
		this.targetIdCard = targetIdCard;
		this.targetBankCard = targetBankCard;
		this.targetBankPhone = targetBankPhone;
		this.targetBankName = targetBankName;
	}
	public TradeCardVO() {
		super();
	}
}