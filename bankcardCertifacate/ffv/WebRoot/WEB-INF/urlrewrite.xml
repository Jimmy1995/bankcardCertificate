<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite use-query-string="true">
    <rule>
    	<note>
        </note>
    	<from>/weixinpay</from>
    	<to>/biz/x2x/wxpay.jsp</to>
    </rule>
    <rule>
    	<note>
        </note>
    	<from>/h5pay/(.*)</from>
    	<to>/x2xpay/jspay.action?brCode=$1</to>
    </rule>
    <rule>
    	<note>
        </note>
    	<from>/callBack/(.*)</from>
    	<to>/x2xpay/payCallBack.action?actionType=$1</to>
    </rule>
</urlrewrite>



